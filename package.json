{
  "name": "motion-canvas-narrator",
  "version": "0.1.0",
  "description": "A Motion Canvas component for handling narration and subtitles with AI provider support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./vite-plugin": "./dist/vite-plugin.js"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "build": "tsc",
    "bundle": "rollup -c rollup.config.mjs",
    "prettier:fix": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src/**/*.ts",
    "eslint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm run clean && npm run bundle"
  },
  "keywords": [
    "motion-canvas",
    "animation",
    "narration",
    "subtitles",
    "text-to-speech"
  ],
  "author": "Patrick Rathje",
  "license": "MIT",
  "peerDependencies": {
    "@motion-canvas/2d": "3.18.0-alpha.0",
    "@motion-canvas/core": "3.18.0-alpha.0"
  },
  "peerDependenciesMeta": {
    "@elevenlabs/elevenlabs-js": {
      "optional": true
    }
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.20.1",
    "path-browserify": "^1.0.1",
    "tsc-alias": "^1.8.16"
  },
  "devDependencies": {
    "@motion-canvas/2d": "^3.18.0-alpha.0",
    "@motion-canvas/core": "^3.18.0-alpha.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-tsdoc": "^0.4.0",
    "prettier": "^2.8.4",
    "prettier-plugin-organize-imports": "^4.2.0",
    "rollup": "^4.50.0",
    "rollup-plugin-dts": "^6.2.3",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0",
    "vite": "^5.4.21"
  }
}
